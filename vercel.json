{
    "version": 2,
    "routes": [
      {
        "src": "/images/(.*)",
        "dest": "/images/$1",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        }
      },
      {
        "src": "/videos/(.*)", 
        "dest": "/videos/$1",
        "headers": {
          "cache-control": "public, max-age=31536000, immutable"
        }
      }
    ]
  }